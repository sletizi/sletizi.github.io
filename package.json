        "vue-router": "^3.1.6",
        "vue-style-loader": "^4.1.2",
        "vue-svgicon": "^3.2.9",
        "vue-tags-ball": "^1.2.0",
        "vuefire": "^3.0.0-alpha.2",
        "vuetify": "^2.2.19"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.2.3",
        "@vue/cli-service": "~4.2.3",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.8.0",
        "eslint-plugin-vue": "^6.2.2",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
        "vue-cli-plugin-vuetify": "~2.0.5",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.4.3"
    },
    "eslintConfig": {
